{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "**/*.@(css|jpg|jpeg|png|gif|js|ico|woff)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=864000"
      }]
    }]
  }
}
